<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
	  <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
	  <Description>Acts as RSBot Library- includes all neccessary logic that the bot and 3rd party applications require.</Description>
	  <Copyright>Copyright ©  2018 - 2024, RSBot Team</Copyright>
	  <PackageProjectUrl>https://github.com/SDClowen/RSBot</PackageProjectUrl>
	  <PackageReadmeFile></PackageReadmeFile>
	  <RepositoryUrl>https://github.com/SDClowen/RSBot</RepositoryUrl>
	  <PlatformTarget>x86</PlatformTarget>
	  <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="LocalizationManager.cs" />
    <Compile Remove="UI\ListViewExtensions.cs" />
    <Compile Remove="UI\MoveDirection.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Avalonia" Version="11.3.0" />
	  <PackageReference Include="Avalonia.ReactiveUI" Version="11.3.0" />
	  <PackageReference Include="PeNet" Version="5.1.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\RSBot.FileSystem\RSBot.FileSystem.csproj" />
    <ProjectReference Include="..\RSBot.NavMeshApi\RSBot.NavMeshApi.csproj" />
  </ItemGroup>

</Project>
